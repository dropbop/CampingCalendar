{
    "version": 2,
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": {
          "cache-control": "s-maxage=31536000,immutable"
        },
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }